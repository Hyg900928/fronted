{
  "name": "ts-react",
  "version": "1.0.0",
  "description": "",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "deploy:dev": "better-npm-run deploy:dev",
    "start:dist": "better-npm-run start:dist",
    "clean": "rimraf dist"
  },
  "betterScripts": {
    "start": {
      "command": "npm run clean && webpack-dev-server --config build/webpack.config.ts",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:dist": {
      "command": "http-server dist",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run clean && ts-node build/compile.ts",
      "env": {
        "NODE_ENV": "development",
        "PORT": "3000",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": " npm run clean && webpack --config build/webpack.config.ts",
      "env": {
        "NODE_ENV": "production",
        "PORT": "3000",
        "DEBUG": "app:*"
      }
    }
  },
